<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog title="Endereços e Contatos">
		<content>
			<f:SimpleForm maxContainerCols="1" editable="false" layout="ResponsiveGridLayout" title="" labelSpanL="2" labelSpanM="3" emptySpanL="1"
				emptySpanM="4" columnsL="1" columnsM="1">
				<Text text="Para alteração de endereço anexar obrigatoriamente comprovante de residência.">
					<layoutData>
						<l:GridData span="L12 M12 S12" linebreakL="true" linebreakM="true" linebreakS="true"/>
					</layoutData>
				</Text>
			</f:SimpleForm>
		</content>
		<endButton>
			<Button text="Fechar" press="onClose"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>